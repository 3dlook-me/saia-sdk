{
  "src": [
    "lib/**/*.js",
    "./jsdoc2md/README.md"
  ],
  "dest": "docs",
  "clean": true,
  "app": {
    "entrance": "content:readme",
    "base": "/jssdkdoc",
    "routing": "query"
  },
  "template": {
    "options": {
      "title": {
        "label": "SAIA JS SDK"
      },
      "navbar": {
        "menu": [
          {
            "label": "README",
            "href": "/jssdkdoc/?content=readme"
          },
          {
            "label": "DOCS",
            "href": "/jssdkdoc/?api"
          }
        ]
      }
    }
  }
}
